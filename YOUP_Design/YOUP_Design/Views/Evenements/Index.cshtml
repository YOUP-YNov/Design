<div class="container divAllEvenement">
    <div class="encartPoub well">
        Encart de publicité
    </div>

    <div class="rechercheEvenement">
        <div class="moduleRechercheEvenement form-group input-group">
            <input type="text" class="form-control" placeholder="Rechercher dans évènement">
        </div>

        <div class="selectCategorieEvent">
            <select class="form-control">
                <option>Catégorie</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
            </select>
        </div>

        <div class="selectRegionEvent">
            <select class="form-control">
                <option>Région</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
            </select>
        </div>

        <div class="selectRegionEvent">
            <select class="form-control">
                <option>Date</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
                <option>5</option>
            </select>
        </div>

        <button type="submit" class="btn btn-primary btnRechercherEvenement">Rechercher</button>

    </div>

    <div class="row triEvenenemt">
        <div class=" col-lg-offset-1 col-lg-2 text-right" >
            Trier par :
        </div>
        <div id="menuEvent" class=" col-lg-6 text-center">
            <div class=" btn-group btn-group-justified menuEvenement">
                <a id="liendisp" class="btn btn-default">Disponibilité</a>
                <a id="lienCate" class="btn btn-default">Catégorie</a>
                <a id="lienDate" class="btn btn-default">Date</a>
            </div>
        </div>
        <div class="col-lg-2">
            <button type="submit" class="btn btn-primary btnRechercherEvenement">Créer un évènement</button>

        </div>
    </div>

