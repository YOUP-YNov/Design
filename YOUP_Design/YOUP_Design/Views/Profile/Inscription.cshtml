@{
    ViewBag.Title = "Inscription";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ActiveMenu = "Profile";
}
<div class="bodyProfil">
    <button type="submit" class="btn btn-primary">Ajouter une photo de profil</button>

    <div class="row">
        <div class="col-lg-2 text-center">
        </div>
        <div class="col-lg-4 col-lg-4Profil text-center border-right border-rightProfil">
            <div class="panel-body">
                <form role="form" class="formProfil">
                    <div class="form-group">
                        <label for="exampleInputPseudo">Pseudo</label>
                        <input type="email" class="form-control" id="exampleInputPSeudo">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail">Email </label>
                        <input type="email" class="form-control" id="exampleInputEmail">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Mot de passe</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Confirmation du mot de passe</label>
                        <input type="password" class="form-control" id="exampleInputPassword2">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputNom">Nom</label>
                        <input type="password" class="form-control" id="exampleInputNom">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPrenom">Prénom</label>
                        <input type="password" class="form-control" id="exampleInputPrenom">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputDate">Date</label>
                        <input type="date" class="form-control" id="exampleInputDate">
                    </div>
                </form>
            </div>
        </div>
        <div class="col-lg-4 col-lg-4Profil text-center">
            <div class="panel-body">
                <form role="form" class="formProfil">
                    <div class="form-group">
                        <label for="exampleInputAdresse">Adresse</label>
                        <input type="password" class="form-control" id="exampleInputAdresse">
                    </div>
                    <iframe class="googleProfil" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2828.402902027907!2d-0.5660659999999815!3d44.854092!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xd55287c2c0f1d0d%3A0x39d74ef785e4ccb7!2sIng%C3%A9sup+Bordeaux!5e0!3m2!1sfr!2sfr!4v1415376131336" width="400" height="280" frameborder="0" style="border:0"></iframe>
                    <div class="form-group">
                        <label>Centres d'intérêts</label>
                        <select multiple="" class="form-control">
                            <option>Sport</option>
                            <option>Visite</option>
                            <option>Apéro entre amis</option>
                            <option>Marche a pied</option>
                        </select>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-lg-2 text-center">
        </div>
    </div>
    <button type="submit" class="btn btn-default btn-defaultProfil">Annuler</button>
    <button type="submit" class="btn btn-primary">Enregistrer</button>
</div>

